{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": []
    },
    "mermaid": {
      "command": "uvx",
      "args": ["mcp-package-mermaid"],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    },
    "excalidraw": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--network",
        "host",
        "-e",
        "EXPRESS_SERVER_URL=http://localhost:3000",
        "-e",
        "ENABLE_CANVAS_SYNC=true",
        "ghcr.io/yctimlin/mcp_excalidraw:latest"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "read_diagram_guide",
        "batch_create_elements",
        "batch_create_elements",
        "export_scene",
        "clear_canvas",
        "set_viewport",
        "describe_scene",
        "get_resource",
        "describe_scene",
        "get_canvas_screenshot"
      ]
    }
  }
}